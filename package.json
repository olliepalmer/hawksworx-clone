    "@11ty/eleventy-plugin-rss": "^1.0.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.0",
    "axios": "^0.18.1",
    "dotenv": "^6.1.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-sass": "^5.0.0",
    "gulp-uglify": "^3.0.1",
    "markdown-it-anchor": "^8.6.4",
    "netlify-lambda": "^1.6.3",
    "sass": "^1.51.0",
    "string": "^3.3.3",
    "twitter": "^1.7.1"
  },
  "devDependencies": {
